{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bit/bit.envs.compilers.react": "file:./components/compilers/react",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "recoil": "0.0.10",
    "typescript": "^3.9.7"
